{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.spider": {
      "arrays": {
        "materials": {
          "Array.materials": [ "Material.default", "Material.invisible" ]
        }
      },
      "geometry": "v.random_costume == 5 ? geometry.spiderwizard:(v.random_costume == 4 ? geometry.spiderwitch:(v.random_costume == 3 ? geometry.spiderkiwi:(v.random_costume == 2 ? geometry.spidercreeper:(Geometry.default))))",
      "materials": [ { "*": "Array.materials[query.is_invisible]" } ],
      "textures": [ "v.random_costume == 5 ? texture.spiderwizard:(v.random_costume == 4 ? texture.spiderwitch:(v.random_costume == 3 ? texture.spiderkiwi:(v.random_costume == 2 ? texture.spidercreeper:(Texture.default))))" ]
    }
  }
}
