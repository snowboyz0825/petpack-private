{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.fox": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.red",
            "Texture.arctic"
          ]
        }
      },
      "geometry": "q.is_name_any('Stardust Fox§1§7') ? geometry.stardust_fox_anim:(q.is_name_any('Stardust Fox§0§7') ? geometry.stardust_fox:(Geometry.default))",
      "part_visibility": [
        {
          "leg*": "!query.is_sleeping"
        },
        {
          "head": "!query.is_sleeping"
        },
        {
          "head_sleeping": "query.is_sleeping"
        }
      ],
      "materials": [
        {
          "*": "q.is_name_any('Stardust Fox§1§7') ? material.fox_anim:(q.is_name_any('Stardust Fox§0§7') ? material.entity_alphablend:(Material.default))"
        }
      ],
      "textures": [
        "q.is_name_any('Stardust Fox§1§7') ? texture.stardust_fox_anim:(q.is_name_any('Stardust Fox§0§7') ? texture.stardust_fox:(Array.skins[query.variant]))"
      ],
      "uv_anim": {
        "offset": [
          0,
          "q.is_name_any('Stardust Fox§1§7') ? (math.mod(math.floor(q.life_time * 2),3) / 3):(0)"
        ],
        "scale": [
          1,
          "q.is_name_any('Stardust Fox§1§7') ? (1/3):(1)"
        ]
      }

    }
  }
}