{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.happy_ghast_snow": {
      "geometry": "q.is_name_any('smol') ? geometry.smol_ghast:(q.is_name_any('Ghastly Pumpkin') ? geometry.ghastlypumpkin:(q.is_name_any('Villager Helicopter§o§m§a§r','Villager Helicopter') ? geometry.villager_copter:(Geometry.default)))",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "q.is_name_any('smol') && v.is_harnessed ? texture.ghastling_saddled:(q.is_name_any('smol') && !v.is_harnessed ? texture.ghastling:(q.is_name_any('Ghastly Pumpkin') ? texture.ghastlypumpkin:(q.is_name_any('Villager Helicopter§o§m§a§r','Villager Helicopter') ? texture.villager_copter:(q.is_baby ? Texture.ghastling : Texture.happy_ghast))))"
      ]
    },
    "controller.render.happy_ghast_ropes": {
      "geometry": "Geometry.ropes",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "q.is_name_any('Villager Helicopter§o§m§a§r','Villager Helicopter') ? texture.blank:(Texture.ropes)"
      ],
      "part_visibility": [
        {
          "*": "variable.render_ropes"
        }
      ]
    }
  }
}
