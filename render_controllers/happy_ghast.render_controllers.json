{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.happy_ghast_snow": {
      "geometry": "query.is_name_any('Villager Helicopter§o§m§a§r') ? geometry.villager_copter:(Geometry.default)",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "query.is_name_any('Villager Helicopter§o§m§a§r') ? texture.villager_copter:(query.is_baby ? Texture.ghastling : Texture.happy_ghast)" ]
    },
    "controller.render.happy_ghast_ropes": {
      "geometry": "Geometry.ropes",
      "materials": [ { "*": "Material.default" } ],
