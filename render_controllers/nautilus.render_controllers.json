{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.nautilus": {
      "geometry": "q.is_name_any('smol') ? geometry.nautilus_smol:(query.is_baby ? Geometry.baby :(Geometry.default))",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "(query.is_baby || q.is_name_any('smol')) ? Texture.baby : Texture.default" ],
      "part_visibility": [
        { "head": "!variable.has_armor" }
      ]
    },
    "controller.render.nautilus_saddle": {
      "geometry": "q.is_name_any('smol') ? geometry.saddle_smol:(Geometry.default)",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.saddle" ],
      "part_visibility": [
        { "*": false },
        { "head": "q.is_saddled" }
      ]
    },
    "controller.render.nautilus_armor": {
      "part_visibility": [
        { "*": false },
        { "head": "variable.has_armor" }
      ],
      "arrays": {
        "textures": {
          "Array.armor": [
            "Texture.armor_none",
            "Texture.armor_copper",
            "Texture.armor_iron",
            "Texture.armor_gold",
            "Texture.armor_diamond",
            "Texture.armor_netherite"
          ],
          "Array.armor_smol": [
            "Texture.armor_none",
            "Texture.armor_copper_smol",
            "Texture.armor_iron_smol",
            "Texture.armor_gold_smol",
            "Texture.armor_diamond_smol",
            "Texture.armor_netherite_smol"
          ]
        }
      },
      "geometry": "q.is_name_any('smol') ? geometry.armor_smol:(Geometry.armor)",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "q.is_name_any('smol') ? Array.armor_smol[variable.armor_texture_slot]:(Array.armor[variable.armor_texture_slot])" ]
    }
  }
}