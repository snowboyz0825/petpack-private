{
    "format_version": "1.8.0",
    "render_controllers": {
      "controller.render.bee": {
        "arrays": {
            "textures": {
              "array.skins": [
                "texture.default",
                "texture.nectar",
                "texture.angry",
                "texture.angry_nectar"
              ]
            }
        },
        "geometry": "query.is_name_any('Wrapped') ? geometry.wrapped : ( query.is_name_any('Invincibee') ? geometry.invincibee : ( query.is_name_any('smol') ? Geometry.smolbee :(Geometry.default)))",
        "materials": [ { "*": "Material.default" } ],
        "textures": [ "query.is_name_any('Wrapped') ? texture.wrapped : ( query.is_name_any('Invincibee') ? texture.invincibee : ( (array.skins[query.property('minecraft:has_nectar') + query.is_angry * 2])))" ]
      }
    }
  }
