{
  "format_version": "1.10.0",
  "render_controllers": {
    "controller.render.copper_golem": {
      "arrays": {
        "textures": {
          "Array.textures": [
            "Texture.default",
            "Texture.exposed",
            "Texture.weathered",
            "Texture.oxidized"
          ]
        }
      },
      "geometry": "q.is_name_any('Jack-O-Lantern') ? geometry.jack_o_Lantern:(q.is_name_any('Sans§u§t') ? geometry.sans:(Geometry.default))",
      "materials": [ { "*":"q.is_name_any('Jack-O-Lantern') ? material.entity_emissive_alpha_one_sided:(q.is_name_any('Sans§u§t') ? material.entity_alphatest:(Material.default))" } ],
      "textures": [ "query.is_name_any('Jack-O-Lantern') ? Texture.jack_o_Lantern:(q.is_name_any('Sans§u§t') ? texture.sans:(q.is_name_any('test') ? texture.test:(Array.textures[v.index])))" ]
    },
    "controller.render.copper_golem_eyes": {
      "arrays": {
        "textures": {
          "Array.textures": [
            "Texture.eyes_default",
            "Texture.eyes_exposed",
            "Texture.eyes_weathered",
            "Texture.eyes_oxidized"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.eyes" } ],
      "textures": [ "Array.textures[v.index]" ],
      "ignore_lighting": true
    },
    "controller.render.copper_golem_flower": {
      "geometry": "Geometry.flower",
      "materials": [ { "*": "Material.flower" } ],
      "textures": [ "Texture.flower" ],
      "part_visibility": [
        { "*": "false" },
        { "flower": "query.property('minecraft:has_flower')" }
      ]
    }
  }
}
