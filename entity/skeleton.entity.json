{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:skeleton",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "skeleton"
      },
      "textures": {
        "default": "textures/entity/skeleton/skeleton",
        "skeletonbunny": "textures/entity/skeleton/skeletonbunny",
        "skeletoncreeper": "textures/entity/skeleton/skeletoncreeper",
        "skeletonwitch": "textures/entity/skeleton/skeletonwitch",
        "skeletonwizard": "textures/entity/skeleton/skeletonwizard"
      },
      "geometry": {
        "default": "geometry.skeleton.v1.8",
        "skeletonbunny":"geometry.skeletonbunny",
        "skeletoncreeper":"geometry.skeletoncreeper",
        "skeletonwitch":"geometry.skeletonwitch",
        "skeletonwizard":"geometry.skeletonwizard"
      },
      "spawn_egg": {
        "texture": "spawn_egg_skeleton"
      },
      "scripts": {
        "initialize": [
          "v.random_costume=math.random_integer(1, 1);" ],
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
          "variable.riding_y_offset_with_respect_to_player = 0.5;",
          "variable.riding_y_offset_on_vehicle_where_other_entites_can_stand = query.is_riding_any_entity_of_type('minecraft:minecart', 'minecraft:boat', 'minecraft:chest_boat') ? -3.0 : 0.0;",
          "variable.riding_y_offset = variable.riding_y_offset_with_respect_to_player + variable.riding_y_offset_on_vehicle_where_other_entites_can_stand;"
        ]
      },
      "animations": {
        "look_at_target_default": "animation.humanoid.look_at_target.default",
        "look_at_target_gliding": "animation.humanoid.look_at_target.gliding",
        "look_at_target_swimming": "animation.humanoid.look_at_target.swimming",
        "move": "animation.humanoid.move",
        "riding.arms": "animation.humanoid.riding.arms",
        "riding.legs": "animation.humanoid.riding.legs",
        "holding": "animation.humanoid.holding",
        "brandish_spear": "animation.humanoid.brandish_spear",
        "charging": "animation.humanoid.charging",
        "attack.rotations": "animation.humanoid.attack.rotations",
        "sneaking": "animation.humanoid.sneaking",
        "bob": "animation.humanoid.bob",
        "damage_nearby_mobs": "animation.humanoid.damage_nearby_mobs",
        "bow_and_arrow": "animation.humanoid.bow_and_arrow",
        "swimming": "animation.zombie.swimming",
        "use_item_progress": "animation.humanoid.use_item_progress",
        "skeleton_attack": "animation.skeleton.attack",
        "riding.body": "animation.humanoid.riding.body"
      },
       "render_controllers": [
        "controller.render.skeleton"
      ],
      "enable_attachables": true
    }
  }
}