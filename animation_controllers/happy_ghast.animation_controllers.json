{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.happy_ghast.move": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            {"move":"!q.is_name_any('smol')"},
            {"move_smol":"q.is_name_any('smol')"}
          ]
        }
      }
    },
    "controller.animation.happy_ghast.custom_always_active": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            {"villager_copter_prop":"q.is_name_any('Villager Helicopter§o§m§a§r','Villager Helicopter')"},
            {"bee_flying":"q.is_name_any('Definitely a Bee')"}
          ]
        }
      }
    },
    "controller.animation.happy_ghast.scale": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            {"scale":"!q.is_name_any('smol')"}
          ]
        }
      }
    }
  }
}
