{
  "format_version": "1.19.0",
  "animation_controllers": {
    "controller.animation.allay.test_allay": {
      "states": {
        "default": {
          "animations": [
            {
              "name_specific_animation_test": "((q.modified_move_speed < 0.2) && q.is_name_any('Test'))"
            }
          ]
        }
      }
    },
    "controller.animation.allay.particles": {
      "states": {
        "default": {
          "animations": [
            {
              "particles_confetti": "q.is_name_any('§s§n§o§Y§G§J§0§T§G§H§j§n§C§e§I','§p§o§p§k§A§S§8§s§l§E§d§L§h§9§c','§s§a§l§o§L§b§Q§V§v§T§y§K§l§c§R','§a§s§t§g§T§U§p§C§M§n§6§m§r§J§p','§d§e§m§k§C§l§j§p§i§A§5§1§F§m§f','§g§a§v§F§i§q§8§x§w§A§u§S§e§I§l','§c§r§z§O§5§K§c§T§Q§t§b§H§L§c§y')"
            },
            {
              "particles_test": "q.is_name_any('Test')"
            },
            {
              "particles_candle_birthday": "q.is_name_any('§s§n§o§Y§G§J§0§T§G§H§j§n§C§e§I','§p§o§p§k§A§S§8§s§l§E§d§L§h§9§c','§s§a§l§o§L§b§Q§V§v§T§y§K§l§c§R','§a§s§t§g§T§U§p§C§M§n§6§m§r§J§p','§d§e§m§k§C§l§j§p§i§A§5§1§F§m§f','§g§a§v§F§i§q§8§x§w§A§u§S§e§I§l','§c§r§z§O§5§K§c§T§Q§t§b§H§L§c§y')"
            }
          ]
        }
      }
    },
    "controller.animation.allay.dancing": {
      "states": {
        "default": {
          "transitions": [
            {
              "missing_state": "q.is_dancing"
            }
          ],
          "blend_transition": 0.2
        },
        "dancing": {
          "animations": [
            {
              "dance": "q.is_name_any('Child§r§u§n§3','The Child§r§u§n§3','Tenna','Angel§r§u§n§3','The Angel§r§u§n§3','Definitely A Bat§c§4§9§9') ? 0:1"
            },
            {
              "child_run_3_dancing": "q.is_name_any('Child§r§u§n§3','The Child§r§u§n§3')"
            },
            {
              "angel_run_3_dancing": "q.is_name_any('Angel§r§u§n§3','The Angel§r§u§n§3')"
            },
            {
              "dance_tenna":"q.is_name_any('Tenna')"
            }
          ],
          "transitions": [
            {
              "default": "!q.is_dancing"
            }
          ]
        }
      }
    },
    "controller.animation.allay.holding": {
      "initial_state": "holding",
      "states": {
        "holding": {
          "animations": [
            {
              "hold_item": "(variable.is_holding_left || variable.is_holding_right) && (q.is_name_any('Tenna') ? (!q.is_dancing):1)"
            }
          ],
          "transitions": [
            {
              "missing_state": "q.modified_move_speed > 0.2"
            }
          ],
          "blend_transition": 0.2
        },
        "fly_holding": {
          "animations": [
            {
              "hold_item_fly": "(variable.is_holding_left || variable.is_holding_right) && (q.is_name_any('Tenna') ? (q.is_dancing ? 0:1):1) && !q.is_name_any('Ed-E')"
            }
          ],
          "transitions": [
            {
              "holding": "q.modified_move_speed < 0.2"
            }
          ],
          "blend_transition": 0.2
        }
      }
    },
    "controller.animation.allay.look_at_target": {
      "states": {
        "default": {
          "transitions": [
            {
              "missing_state": "!q.is_dancing"
            }
          ]
        },
        "looking_at_target": {
          "animations": [
            "look_at_target_default"
          ],
          "transitions": [
            {
              "default": "q.is_dancing"
            }
          ]
        }
      }
    },
    "controller.animation.allay.general": {
      "initial_state": "idling",
      "states": {
        "idling": {
          "animations": [
            {
              "idle": "(q.is_name_any('Child§r§u§n§3','The Child§r§u§n§3','Angel§r§u§n§3','The Angel§r§u§n§3','Definitely A Bat§c§4§9§9') ? 0:1) && (q.is_name_any('Tenna') ? (!q.is_dancing):1) || q.is_name_any('Ed-E')"
            }
          ],
          "transitions": [
            {
              "missing_state": "q.modified_move_speed > 0.2"
            }
          ],
          "blend_transition": 0.2
        },
        "flying": {
          "animations": [
            {
              "fly": "!q.is_name_any('Definitely a Bee','Definitely A Bat§c§4§9§9','Ed-E')"
            }
          ],
          "transitions": [
            {
              "idling": "q.modified_move_speed < 0.2"
            }
          ],
          "blend_transition": 0.2
        }
      }
    },
    "controller.animation.allay.custom_always_active": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            {
              "bee_flying": "q.is_name_any('Definitely a Bee')"
            },
            {
              "bee_fly_bobbing": "q.is_name_any('Definitely a Bee')"
            },
            {
              "ophanim": "q.is_name_any('Ophanim')"
            },
            {
              "child_run_3_idle": "q.is_name_any('Child§r§u§n§3','The Child§r§u§n§3') && q.is_dancing == 0"
            },
            {
              "angel_run_3_idle": "q.is_name_any('Angel§r§u§n§3','The Angel§r§u§n§3') && q.is_dancing == 0"
            },
            {
              "wither_storm": "q.is_name_any('Wither Storm§s§t§o§r§y§m§o§d§e')"
            },
            {
              "bat_fly": "q.is_name_any('Definitely A Bat§c§4§9§9')"
            }
          ]
        }
      }
    }
  }
}
